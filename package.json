{
  "name": "youtube-downloader",
  "description": "Download YouTube videos easily",
  "license": "MIT",
  "private": false,
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Santosl2/youtube-downloader.git"
  },
  "type": "module",
  "main": "public/electron.cjs",
  "homepage": "./",
  "scripts": {
    "dev-vite": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "dev": "concurrently -k \"BROWSER=none npm run dev-vite\" \"npm:electron\"",
    "electron": "wait-on tcp:5173 && electron .",
    "package": "npm run build && electron-forge package",
    "make-mac": "npm run build && electron-forge make --platform darwin",
    "make-linux": "npm run build && electron-forge make --platform linux",
    "make": "npm run build && electron-forge make"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.8",
    "@mui/styled-engine-sc": "^5.10.6",
    "axios": "^1.1.0",
    "cors": "^2.8.5",
    "electron": "^21.1.0",
    "electron-is-dev": "^2.0.0",
    "express": "^4.18.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6",
    "youtube-pl": "^2.0.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-dmg": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@vitejs/plugin-react": "^2.1.0",
    "concurrently": "^7.4.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.8",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "vite": "^3.1.0",
    "wait-on": "^6.0.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "youtube_playlist_downloader"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "name": "youtube_playlist_downloader"
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {
            "name": "youtube_playlist_downloader"
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "name": "youtube_playlist_downloader",
            "format": "ULFO"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "linux",
            "win32"
          ]
        }
      ]
    }
  }
}
